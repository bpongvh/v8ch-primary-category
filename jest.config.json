{
	"collectCoverageFrom": [
		"src/**/*.js",
		"!**/node_modules/**",
		"!**/test/**"
	],
	"moduleDirectories": ["node_modules"],
	"preset": "@wordpress/jest-preset-default",
	"testMatch": ["**/test/**/*.js?(x)", "**/?(*.)+(spec|test).js?(x)"],
	"setupFiles": ["./test/js/setup/globals"],
	"testPathIgnorePatterns": [
		"/node_modules/",
		"/test/js/mocks/",
		"/test/js/setup/"
	]
}
